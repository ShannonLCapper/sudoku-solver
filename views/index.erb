<h3>Fill in any known tiles in the template below</h3>
<form id="sudokuForm" action="/solution" method="post" autocomplete="off">
	<table>
		<% 9.times do |row| %>
			<tr>
				<% 9.times do |column| %>
					<td><input class="slot" type="text" pattern="[0-9]*" maxlength="1" name=<%= row.to_s + "-" + column.to_s %> value=""></td>
				<% end %>
			</tr>
		<% end %>
	</table>

	<p id="message"></p>
	<button type="submit">Solve</button>
	<button type="reset">Reset</button>
</form>